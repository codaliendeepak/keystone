// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model Client {
  id              String  @id @default(cuid())
  name            String  @default("")
  description     String  @default("")
  photo_filesize  Int?
  photo_extension String?
  photo_width     Int?
  photo_height    Int?
  photo_id        String?
  status          String? @default("CURRENT")
  email           String  @unique @default("")
  phone           Int
}

model Faq {
  id       String @id @default(cuid())
  Question String @default("")
  ans      String @default("")
}

model Content {
  id              String  @id @default(cuid())
  pagename        String?
  image_filesize  Int?
  image_extension String?
  image_width     Int?
  image_height    Int?
  image_id        String?
  type            String? @default("Cards")
  description     String  @default("")
  heading         String  @default("")
  Section         String? @default("Section1")
}

model FormQuery {
  id               String  @id @default(cuid())
  name             String  @default("")
  type             String? @default("Buisness")
  description      String  @default("")
  buisnessCategory String  @default("")
  email            String  @unique @default("")
  packagesCount    String  @default("")
  subject          String  @default("")
  phone            Int     @default(0)
}

model JobRole {
  id                  String  @id @default(cuid())
  Role                String  @default("")
  employmenttype      String? @default("Full Time Employee (FTE)")
  jobType             String? @default("Product")
  jobResponsilbilties String  @default("")
  jobRequirements     String  @default("")
  jobLocation         String  @default("")
}

model JobApplication {
  id          String  @id @default(cuid())
  name        String  @default("")
  email       String  @default("")
  jobRole     String  @default("")
  coverLetter String  @default("")
  CV_filesize Int?
  CV_filename String?

  @@index([email])
}

model User {
  id        String    @id @default(cuid())
  name      String    @default("")
  email     String    @unique @default("")
  password  String?
  createdAt DateTime?
  isAdmin   Boolean   @default(false)
}
